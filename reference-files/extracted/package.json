{
  "name": "@thoughtspot/dyoc",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "nx-install": "pnpm run build",
    "build": "npm-run-all build:clean build:ts copy-css copy-fonts build:dyocsdk copy-dyocsdk",
    "build:ts": "tsc -p .",
    "build:clean": "rimraf lib tsconfig.tsbuildinfo src/dyocsdk/dist",
    "copy-css": "copyfiles -u 1 src/**/*.*css lib",
    "copy-fonts": "copyfiles -u 1 src/**/*.*tf lib",
    "copy-dyocsdk": "cp -r src/dyocsdk/dist lib/dyocsdk",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' '--fix'",
    "biomelint": "biome lint src/**",
    "tsc": "tsc -p . --incremental false --noEmit true",
    "ttsc": "ttsc -p . --noEmit false",
    "ttsw": "ttsc -p . -w --noEmit false",
    "tsw": "tsc -p . -w --declaration false",
    "copyfiles": "copyfiles",
    "bundle": "npm run build:dyocsdk && vite build",
    "dev": "npm run build:dyocsdk && vite --port 3002",
    "serve": "vite serve",
    "build:dyocsdk": "vite build --config vite.dyocsdk.config.js",
    "vercel:deploy:prod": "npm run bundle && vercel pull && vercel build --prod && vercel deploy --prebuilt --prod",
    "vercel:deploy:preview": "npm run bundle && vercel pull && vercel build && vercel deploy --prebuilt"
  },
  "license": "ISC",
  "dependencies": {
    "@thoughtspot/ts-chart-sdk": "1.2.4",
    "@viz/muze": "4.7.5",
    "js-base64": "^2.5.1",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.25.2",
    "split.js": "1.6.5"
  },
  "devDependencies": {
    "@types/js-base64": "2.3.1",
    "@types/lodash": "4.17.7",
    "@types/node": "18.16.3",
    "copyfiles": "^2.2.0",
    "eslint": "8.40.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-ban": "^1.5.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-modules": "^3.2.2",
    "rimraf": "^3.0.0",
    "ts-auto-mock": "^2.6.4",
    "ttypescript": "^1.5.15",
    "typescript": "4.8.4",
    "typescript-transform-paths": "^2.2.2",
    "vite": "4.2.1"
  }
}